                        <Font name="Arial" size="12.0" />
                    </font>
                </Button>
                <Button fx:id="btnBackup" layoutX="14.0" layoutY="371.0" mnemonicParsing="false" onMouseClicked="#doBackup" text="Backup now" />
            <Label layoutX="7.0" layoutY="197.0" prefHeight="17.0" prefWidth="135.0" text="Restore">
