<VBox alignment="CENTER" fillWidth="false" prefHeight="481.0" prefWidth="729.0" spacing="20.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="net.ayoentem.storagemanager.main.MainController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <children>
        <AnchorPane prefHeight="441.0" prefWidth="953.0">
            <children>
                <ListView fx:id="chartList" layoutX="287.0" layoutY="197.0" onMouseClicked="#clickedON" prefHeight="218.0" prefWidth="388.0" />
                <Label alignment="CENTER" layoutY="-2.0" prefHeight="32.0" prefWidth="206.0" text="Storage Manager">
                    <font>
                        <Font name="Arial Bold" size="21.0" />
                    </font>
                </Label>
                <Label alignment="CENTER" layoutX="414.0" layoutY="173.0" prefHeight="17.0" prefWidth="135.0" text="Drivers" textAlignment="CENTER">
                    <font>
                        <Font name="Arial Bold" size="15.0" />
                    </font>
                </Label>
                <Label layoutX="7.0" layoutY="59.0" prefHeight="17.0" prefWidth="135.0" text="Backup">
                    <font>
                        <Font name="Arial Bold" size="15.0" />
                    </font>
                </Label>
                <TextField fx:id="txtPathData" layoutX="14.0" layoutY="88.0" prefHeight="25.0" prefWidth="350.0" promptText="Path from Datas" />
