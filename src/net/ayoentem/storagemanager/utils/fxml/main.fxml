            </Label>
            <ChoiceBox fx:id="listBackups" layoutX="7.0" layoutY="294.0" prefWidth="150.0" />
            <Button fx:id="btnChoose11" layoutX="177.0" layoutY="295.0" mnemonicParsing="false" text="Restore">
               <font>
                  <Font name="Arial" size="12.0" />
               </font>
            </Button>
            <Label layoutX="14.0" layoutY="419.0" prefHeight="17.0" prefWidth="447.0" textFill="RED">
               <font>
                  <Font name="Arial" size="12.0" />
               </font>
            </Label>
            <Label fx:id="lblLastBackup" contentDisplay="TEXT_ONLY" focusTraversable="false" layoutX="14.0" layoutY="146.0" prefHeight="17.0" prefWidth="256.0" text="Last Backup:  ">
